def my_function(n,reverse):
    original=n
    while n>0:
        digit=n%10
        reverse=reverse*10+digit
        n//=10
    if original==reverse:
        print("palindrome")
    else:
        print("not palindrome")
n=int(input("enter the number:"))
reverse=0
my_function(n,reverse)

output:
enter the number:121
palindrome
